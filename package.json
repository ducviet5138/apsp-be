  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"shared_resources/**/*.ts\" \"migrations/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,shared_resources,migrations}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky && husky install",
    "create:migration": "node -e \"require('child_process').execSync('typeorm migration:create ./migrations/' + process.argv[1], {stdio: 'inherit'})\"",
    "migrate": "npx typeorm-ts-node-commonjs migration:run -d typeorm.config.ts"
  },
